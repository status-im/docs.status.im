	<!-- DEVELOPMENT-CONTRIBUTE-TO-STATUS -->
	<section class="development-contribute-to-status p-t-160 p-b-160" id="contribute-to-status">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<h2>Contribute to Status</h2>
					<p class="teaser-2">Get set up with this Contributor Guide. <a href="/docs/contributor_guide.html" class="link-arrow quick-start-guide">Quick Start Guide</a></p>
				</div>
			</div>
		</div>
		<div class="container">
			<div class="row">
				<div class="col-md-6">
					<div class="box-with-icon m-t-50">
						<%- image_tag("/img/github-black-logo.svg") %>
						<div class="inner">
							<h3>Status-React</h3>
							<p class="p-large">Written in Clojure - this is where we handle interactions between Whisper and the app; between the wallet, the user and any Distributed Application (DApp) that they want to access; and where the UI logic lives.</p>
							<a href="https://github.com/status-im/status-react" target="_blank" class="link-arrow">View Repository</a>
						</div>
					</div>
				</div>
				<div class="col-md-6">
					<div class="box-with-icon m-t-50">
						<%- image_tag("/img/github-black-logo.svg") %>
						<div class="inner">
							<h3>Status-Go</h3>
							<p class="p-large">Written in Go - this is where all of the blockchain logic, transactions, and light client work we do lives. </p>
							<a href="https://github.com/status-im/status-go" target="_blank" class="link-arrow">View Repository</a>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="container m-t-90 open-issues">
			<div class="row">
